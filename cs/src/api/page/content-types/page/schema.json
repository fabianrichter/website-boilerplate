{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "singularName": "page",
    "pluralName": "pages",
    "displayName": "Page",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "components": {
      "type": "dynamiczone",
      "components": [
        "general.accordeon",
        "general.page-title",
        "general.section-title",
        "general.text",
        "general.image",
        "connect.article-overview"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "title": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "slug": {
      "type": "string",
      "regex": "[a-z0-9\\-\\/]",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    }
  }
}
