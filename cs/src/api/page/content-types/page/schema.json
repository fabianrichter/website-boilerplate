{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "singularName": "page",
    "pluralName": "pages",
    "displayName": "Page",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "components": {
      "type": "dynamiczone",
      "components": [
        "text.page-title",
        "text.section-title",
        "text.text",
        "media.image",
        "connect.article-overview",
        "forms.contact-form",
        "text.text-and-image",
        "media.gallery",
        "general.intro",
        "general.portfolio",
        "text.hero-text",
        "links.cta",
        "links.download-overview",
        "text.accordeon"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "title": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "slug": {
      "type": "string",
      "regex": "[a-z0-9\\-\\/]",
      "required": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "unique": false
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    }
  }
}
